{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "installCommand": "npm install --no-optional",
    "outputDirectory": "dist",
    "framework": "vite",
    "cleanUrls": true,
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "github": {
        "silent": true
    },
    "env": {
        "NPM_FLAGS": "--no-optional"
    }
}